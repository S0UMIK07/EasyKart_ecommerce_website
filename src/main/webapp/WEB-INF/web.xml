<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
   version="6.0">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductOperationServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.ProductOperationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeliveryAddressServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.DeliveryAddressServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.OrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminOrderOperationServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.AdminOrderOperationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUser</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.EditUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProductServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.EditProductServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pay</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.pay</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pay1</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.pay1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditPaymentByUserServlet</servlet-name>
        <servlet-class>com.mycompany.easykart.servlets.EditPaymentByUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductOperationServlet</servlet-name>
        <url-pattern>/ProductOperationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeliveryAddressServlet</servlet-name>
        <url-pattern>/DeliveryAddressServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderServlet</servlet-name>
        <url-pattern>/OrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminOrderOperationServlet</servlet-name>
        <url-pattern>/AdminOrderOperationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUser</servlet-name>
        <url-pattern>/EditUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProductServlet</servlet-name>
        <url-pattern>/EditProductServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pay</servlet-name>
        <url-pattern>/pay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pay1</servlet-name>
        <url-pattern>/pay1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditPaymentByUserServlet</servlet-name>
        <url-pattern>/EditPaymentByUserServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
